{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "indent": ["error", "tab", { "SwitchCase": 1 }],
        "no-tabs": 0
    }
}
